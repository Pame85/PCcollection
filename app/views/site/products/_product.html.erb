<div id="<%= dom_id(product) %>">
  <p>
    <strong>Category:</strong>
    <%= product.category_id %>
  </p>
  <p>
    <strong>Vendor:</strong>
    <%= product.vendor_id %>
  </p>
  <p>
    <strong>Sku:</strong>
    <%= product.sku %>
  </p>
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>
  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>
  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>
  <p>
    <strong>Available:</strong>
    <%= product.available %>
  </p>
</div>